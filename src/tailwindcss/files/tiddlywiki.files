{
  "tiddlers": [
    {
      "file": "./styles.min.css",
      "fields": {
        "title": "styles.min.css",
        "type": "text/css",
        "description": "Tailwindcss Styles",
        "tags": "$:/tags/Stylesheet",
        "hide-body": "yes"
      }
    }
  ],
  "directories": [
    {
      "path": "docs",
      "filesRegExp": "^.*\\.(html)$",
      "fields": {
        "title": {
          "source": "basename",
          "prefix": "$:/plugins/oeyoews/tailwindcss/docs/"
        },
        "caption": {
          "source": "basename"
        }
      }
    },
    {
      "path": "docs",
      "filesRegExp": "^.*\\.(tid)$",
      "fields": {
        "title": {
          "source": "basename",
          "prefix": "$:/plugins/oeyoews/tiddlywiki-tailwindcss/docs/"
        },
        "caption": {
          "source": "basename"
        }
      }
    }
  ]
}
